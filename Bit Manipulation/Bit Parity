#include <iostream>
using namespace std;

typedef long long ll;

// Function to check the parity of the bits
bool bitparity(ll n) {
    return __builtin_parityll(n);
}

int main() {
    ll n = 15; // Example number
    cout << "Bit parity of " << n << " is: " << bitparity(n) << endl;
    return 0;
}

#include <iostream>
using namespace std;

typedef long long ll;

// Function to count leading zero bits
ll leadingZeroBits(ll n) {
    return __builtin_clzll(n);
}

int main() {
    ll n = 15; // Example number
    cout << "Leading zero bits in " << n << " is: " << leadingZeroBits(n) << endl;
    return 0;
}

#include <iostream>
using namespace std;

typedef long long ll;

// Function to find the position of the first set bit
ll firstSetBit(ll n) {
    return __builtin_ffsll(n);
}

int main() {
    ll n = 15; // Example number
    cout << "First set bit in " << n << " is at position: " << firstSetBit(n) << endl;
    return 0;
}

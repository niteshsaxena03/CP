#include <iostream>
using namespace std;

typedef long long ll;

// Function to count trailing zero bits
ll trailingZeroBits(ll n) {
    return __builtin_ctzll(n);
}

int main() {
    ll n = 15; // Example number
    cout << "Trailing zero bits in " << n << " is: " << trailingZeroBits(n) << endl;
    return 0;
}
